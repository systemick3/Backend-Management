    <body>
        <div id="page" class="front">
            
            <div class="row" id="header">
                <div class="header-logo col-md-2">
                    <a>
                        <img src="/bundles/intechnologybackendmanagement/assets/images/svg/logo.svg" alt="inTechnology wifi">
                    </a>
                </div>
                {% if is_granted('ROLE_USER') %}
                <div class="header-buttons col-md-2">
                    <ul>
                        <li class="nav-btn nav-btn-user">
                            <a href="#">{{ render(controller('IntechnologyBackendManagementBundle:Default:fullName')) }}</a>
                            <i class="fa fa-user"></i>
                        </li>
                        <li class="nav-btn nav-btn-logout">
                            <a href="/logout">Logout</a>
                            <i class="fa fa-close"></i>
                        </li>
                    </ul>
                </div>
                {% endif %}
            </div>
            
            <div class="row page-body">
                {% block int_login_content %}
                {% endblock int_login_content %}
                
                {% if is_granted('ROLE_USER') %}
                <div class="col-md-2" id="sidebar">
                    <ul class="nav nav-pills nav-stacked">
                        <li class="nav-btn nav-btn-status">
                            <a href="/" class="first">
                                <div><i class="fa fa-tachometer fa-5x"></i></div>
                                <span>Status</span>
                            </a>
                        </li>
                        <li class="nav-btn nav-btn-companies">
                            <a href="/companies">
                                <div><i class="fa fa-building fa-5x"></i></div>
                                <span>Companies</span>
                            </a>
                        </li>
                        <li class="nav-btn nav-btn-deployments">
                            <a href="/deployments">
                                <div><i class="fa fa-cog fa-5x"></i></div>
                                <span>Deployments</span>
                            </a>
                        </li>
                        <li class="nav-btn nav-btn-users">
                            <a href="/users">
                                <div><i class="fa fa-user fa-5x"></i></div>
                                <span>Users</span>
                            </a>
                        </li>
                    </ul>
                </div>
                {% endif %}
                <div class="col-md-2" id="main-content">
                    <div>
                        {#
                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                            {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                            <a href="{{ path('fos_user_security_logout') }}">
                                {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                            </a>
                        {% else %}
                            <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
                        {% endif %}
                        #}
                    </div>

                    {#
                    {% block fos_user_content %}
                    {% endblock fos_user_content %}
                    #}
                    {% block int_bm_content %}
                    {% endblock int_bm_content %}
                </div>
            </div>
            <div class="row">
                <footer class="footer  one-whole  text-center  pos-fixed  pos-bottom">
                    <small>&copy; 2014 intechnology WiFi, All Rights Reserved.</small>
                </footer>
            </div>
        </div>

