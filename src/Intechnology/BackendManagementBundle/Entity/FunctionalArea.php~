<?php

namespace Intechnology\BackendManagementBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * FunctionalArea
 */
class FunctionalArea
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $name;

    /**
     * @var string
     */
    private $navigationClass;

    /**
     * @var string
     */
    private $active;

    /**
     * @var string
     */
    private $deleted;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return FunctionalArea
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set navigationClass
     *
     * @param string $navigationClass
     * @return FunctionalArea
     */
    public function setNavigationClass($navigationClass)
    {
        $this->navigationClass = $navigationClass;

        return $this;
    }

    /**
     * Get navigationClass
     *
     * @return string 
     */
    public function getNavigationClass()
    {
        return $this->navigationClass;
    }

    /**
     * Set active
     *
     * @param string $active
     * @return FunctionalArea
     */
    public function setActive($active)
    {
        $this->active = $active;

        return $this;
    }

    /**
     * Get active
     *
     * @return string 
     */
    public function getActive()
    {
        return $this->active;
    }

    /**
     * Set deleted
     *
     * @param string $deleted
     * @return FunctionalArea
     */
    public function setDeleted($deleted)
    {
        $this->deleted = $deleted;

        return $this;
    }

    /**
     * Get deleted
     *
     * @return string 
     */
    public function getDeleted()
    {
        return $this->deleted;
    }
}
